    "source_file": "technieken_index.json",
    "enforce_ids_exist": true
  },
  "normalization": {
    "lowercase": true,
    "strip_diacritics": true,
    "remove_punctuation": true,
    "collapse_whitespace": true
  },
  "matching": {
    "mode": "substring",
    "case_insensitive": true
  },
  "scoring": {
    "anchor_weight": 5,
    "support_weight": 1,
    "min_score_to_consider": 5,
    "leaf_dominance_margin": 6
  },
